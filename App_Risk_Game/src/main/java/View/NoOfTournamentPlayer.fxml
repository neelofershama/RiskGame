        <ComboBox fx:id="option1" prefWidth="150.0" layoutX="30.0" layoutY="150.0"  />
        <ComboBox fx:id="option2" prefWidth="150.0" layoutX="100.0" layoutY="150.0" />
        <ComboBox fx:id="option3" prefWidth="150.0" layoutX="170.0" layoutY="150.0" />
        <ComboBox fx:id="option4" prefWidth="150.0" layoutX="240.0" layoutY="150.0" />

        <Button fx:id="submit_no_players" layoutX="84.0" layoutY="273.0" mnemonicParsing="false" text="submit" onAction="#onclickedNoOfPlayers" />
